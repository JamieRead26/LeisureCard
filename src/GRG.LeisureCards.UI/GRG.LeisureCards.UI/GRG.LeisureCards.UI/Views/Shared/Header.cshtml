<header>    
	<div class="row">
		 
        <a href="/#/" class="logo">Leisure Card</a>

        <div class="end-date" ng-if="$storage.user">
            End Date <span ng-bind="$storage.user.RenewalDate | date:'MM/dd/yyyy'"></span>
        </div>
       
		<div class="clear"></div>
		
    </div>
</header>

<div class="container auth" ng-if="$storage.user">
    <div class="row">
        <div class="sixcol">
            <ol class="ab-nav breadcrumb">
                <li ng-repeat="breadcrumb in breadcrumbs.get() track by breadcrumb.path" ng-class="{ active: $last }">
                    <a ng-if="!$last" ng-href="#{{ breadcrumb.path }}" ng-bind="breadcrumb.label" class="margin-right-xs"></a>
                    <span ng-if="$last" ng-bind="breadcrumb.label"></span>
                </li>
            </ol>
        </div>
        <div class="sixcol last">
            <a href="/#/logout">Logout</a>
        </div>
        <div class="clear"></div>
    </div>
</div>
